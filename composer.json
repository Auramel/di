{
    "name": "yii/di",
    "type": "library",
    "description": "Yii DI container",
    "keywords": [],
    "homepage": "http://www.yiiframework.com/",
    "license": "MIT",
    "require": {
        "php": "^7.1.0",
        "psr/container": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "provide": {
        "psr/container-implementation": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "yii\\di\\": "src",
            "yii\\di\\tests\\": "tests"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    }
}
